{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "types": ["cypress", "@testing-library/cypress"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
, "jest.config.ts", "stabs/doctors.ts", "tests/components/SpecialityFiltration.test.tsx", "tests/SearchField.test.tsx", "tests/components/ListOfDoctors.test.tsx", "tests/components/NavBar.test.tsx", "tests/components/DoctorItem.test.tsx", "tests/components/AppointmentForm.test.tsx", "tests/setupTests.ts"  ]
}
